{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "ES2021",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,
    "strictNullChecks": false,
    "noImplicitAny": false,
    "strictBindCallApply": false,
    "forceConsistentCasingInFileNames": false,
    "noFallthroughCasesInSwitch": false,
    "paths": {
      "@api/*": [
        "apps/core-api/src/*"
      ],
      "@app/storage": [
        "libs/storage/src"
      ],
      "@app/storage/*": [
        "libs/storage/src/*"
      ],
      "@app/core-enum": [
        "libs/core-enum/src"
      ],
      "@app/core-enum/*": [
        "libs/core-enum/src/*"
      ],
      "@app/user": [
        "libs/user/src"
      ],
      "@app/user/*": [
        "libs/user/src/*"
      ],
      "@app/product": [
        "libs/product/src"
      ],
      "@app/product/*": [
        "libs/product/src/*"
      ],
      "@app/auth": [
        "libs/auth/src"
      ],
      "@app/auth/*": [
        "libs/auth/src/*"
      ],
      "@app/inventory": [
        "libs/inventory/src"
      ],
      "@app/inventory/*": [
        "libs/inventory/src/*"
      ],
      "@app/transaction": [
        "libs/transaction/src"
      ],
      "@app/transaction/*": [
        "libs/transaction/src/*"
      ]
    }
  }
}