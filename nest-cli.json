{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/core-api/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/core-api/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/core-api",
  "core-api": {
    "type": "application",
    "root": "apps/core-api",
    "entryFile": "main",
    "sourceRoot": "apps/core-api/src",
    "compilerOptions": {
      "tsConfigPath": "apps/core-api/tsconfig.app.json"
    }
  },
  "storage": {
    "type": "library",
    "root": "libs/storage",
    "entryFile": "index",
    "sourceRoot": "libs/storage/src",
    "compilerOptions": {
      "tsConfigPath": "libs/storage/tsconfig.lib.json"
    }
  },
  "core-enum": {
    "type": "library",
    "root": "libs/core-enum",
    "entryFile": "index",
    "sourceRoot": "libs/core-enum/src",
    "compilerOptions": {
      "tsConfigPath": "libs/core-enum/tsconfig.lib.json"
    }
  },
  "projects": {
    "user": {
      "type": "library",
      "root": "libs/user",
      "entryFile": "index",
      "sourceRoot": "libs/user/src",
      "compilerOptions": {
        "tsConfigPath": "libs/user/tsconfig.lib.json"
      }
    },
    "product": {
      "type": "library",
      "root": "libs/product",
      "entryFile": "index",
      "sourceRoot": "libs/product/src",
      "compilerOptions": {
        "tsConfigPath": "libs/product/tsconfig.lib.json"
      }
    },
    "auth": {
      "type": "library",
      "root": "libs/auth",
      "entryFile": "index",
      "sourceRoot": "libs/auth/src",
      "compilerOptions": {
        "tsConfigPath": "libs/auth/tsconfig.lib.json"
      }
    },
    "inventory": {
      "type": "library",
      "root": "libs/inventory",
      "entryFile": "index",
      "sourceRoot": "libs/inventory/src",
      "compilerOptions": {
        "tsConfigPath": "libs/inventory/tsconfig.lib.json"
      }
    },
    "transaction": {
      "type": "library",
      "root": "libs/transaction",
      "entryFile": "index",
      "sourceRoot": "libs/transaction/src",
      "compilerOptions": {
        "tsConfigPath": "libs/transaction/tsconfig.lib.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/exception",
      "entryFile": "index",
      "sourceRoot": "libs/exception/src",
      "compilerOptions": {
        "tsConfigPath": "libs/exception/tsconfig.lib.json"
      }
    }
  }
}